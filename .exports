export EDITOR='subl -w'

PATH=/usr/local/share/npm/bin:$PATH
PATH=/usr/local/sbin:$PATH
PATH=/usr/local/bin:$PATH
PATH=/db/bin:$PATH
PATH="$(brew --prefix coreutils)/libexec/gnubin:$PATH"
PATH="$(brew --prefix make)/libexec/gnubin:$PATH"
PATH=/usr/local/opt/go/libexec/bin:$PATH
PATH=/usr/local/opt/python/libexec/bin:$PATH
PATH=/usr/local/opt/openjdk/bin:$PATH
PATH=/Applications/Postgres.app/Contents/Versions/latest/bin:$PATH
PATH=~/.composer/vendor/bin:$PATH

PYENV_ROOT="$HOME/.pyenv"
PATH="$PYENV_ROOT/.shims:$PYENV_ROOT/bin:$PATH"

export PATH

export PYENV_ROOT

export CDPATH=".:/www:/ds"

export MANPATH=$MANPATH:/opt/local/share/man

export PYTHONPATH=$PYTHONPATH:.

export NODE_PATH=/usr/local/lib/node_modules:/usr/local/share/npm/lib/node_modules:$NODE_PATH

export DOCKER_HOST=tcp://127.0.0.1:4243

export HOMEBREW_CASK_OPTS="--appdir=/Applications"
export HOMEBREW_NO_ANALYTICS=1

# Specifically for Markbotâ€™s out-dated requirements
export JAVA_HOME=/Library/Java/JavaVirtualMachines/adoptopenjdk-11.jdk/Contents/Home

source ~/.exports-private
